{
  "name": "traefiktop",
  "author": "darksworm (Ilmars Janis Bluzmanis)",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun src/main.tsx",
    "logs": "bun tools/logs.ts --session=latest",
    "build:node": "rollup -c rollup.config.js --configPlugin typescript2",
    "build:binary": "bun build --compile --minify --sourcemap src/main.tsx",
    "tsx": "tsx src/main.tsx",
    "lint": "biome check",
    "lint:fix": "biome check --write",
    "format": "biome format --write",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "test:coverage": "bun test --coverage --coverage-reporter=lcov",
    "test:ci": "bun test --coverage --coverage-reporter=lcov"
  },
  "description": "A TUI for visualizing Traefik routing.",
  "engines": {
    "node": ">=18",
    "bun": ">=1.2.20"
  },
  "bugs": {
    "url": "https://github.com/darksworm/traefiktop/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/darksworm/traefiktop.git"
  },
  "license": "GPL-3.0-or-later",
  "dependencies": {
    "immer": "^10.1.1",
    "ink": "^6.2.1",
    "ink-text-input": "^6.0.0",
    "neverthrow": "^8.2.0",
    "pino": "^9.9.0",
    "react": "^19.1.1",
    "yup": "^1.7.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.2",
    "@rollup/plugin-json": "^6.1.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "ink-testing-library": "^4.0.0",
    "pino-pretty": "^13.1.1",
    "rollup": "^4.46.4",
    "rollup-plugin-typescript2": "^0.36.0",
    "tsx": "^4.20.5"
  },
  "keywords": [
    "traefik",
    "tui",
    "terminal-ui",
    "text-ui",
    "react-tui",
    "cli"
  ]
}
